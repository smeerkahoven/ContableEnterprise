	<parameter name="ID_DEVOLUCION" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select de.id_devolucion
, cc.id_numero_gestion
, cc.gestion
, de.id_cliente
, cl.nombre as cliente
, case de.moneda when 'B' then 'BOLIVIANOS' when 'U' then 'DOLARES' end moneda
, de.id_nota_debito
, de.id_pago_anticipado
, de.concepto
, de.fecha_emision
, de.moneda
, de.factor_cambiario
, number_to_string(de.monto) literal
, round(abs(mod(de.monto,1)*100)) decimal_part
, coalesce(de.monto,0) monto_devolucion
, de.concepto
, de.tipo_devolucion
, case de.tipo_devolucion 	
		when 'E' then 'CONTADO EFECTIVO' 
		when 'H' then concat('CONTADO con CHEQUE Nro :',de.nro_cheque, ' en la Cuenta: ',coalesce( pc.cuenta,'SIN NOMBRE'))
	end forma_pago_literal
, de.fecha_insert
, de.id_usuario_creador
, case de.estado when 'A' then 'ANULADO' when 'E' then 'EMITIDO' end estado

from cnt_devolucion de
inner join cnt_cliente cl on cl.id_cliente = de.id_cliente
left join cnt_pago_anticipado pa on pa.id_pago_anticipado = de.id_pago_anticipado
left join cnt_plan_cuentas pc on pc.id_plan_cuentas = de.id_cuenta_deposito
left join cnt_comprobante_contable cc on cc.id_devolucion = de.id_devolucion and cc.tipo='CE'
WHERE de.id_devolucion = $P{ID_DEVOLUCION}]]>
	</queryString>
