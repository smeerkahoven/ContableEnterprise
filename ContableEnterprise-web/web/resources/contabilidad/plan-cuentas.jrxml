	<queryString language="SQL">
		<![CDATA[select * from (
select pc.id_empresa, 
em.razon_social, em.tipo,
convert (pc.id_plan_cuenta_padre ,char(16)) as id_plan_cuenta_padre , 
convert (pc.id_plan_cuentas, char(16)) as id_plan_cuentas,
